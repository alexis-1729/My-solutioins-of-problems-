#include <bits/stdc++.h>

using namespace std;
int n;
priority_queue<long long int>pq;
main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);
    cin>>n;
    while(n--){
        long long int x; cin>>x;
        pq.push(x);
        if(x==0)
        {
           long long  int y=pq.top();
        cout<<y<<" ";pq.pop();
        }

    }

}
