#include <bits/stdc++.h>


using namespace std;
int n;long long int vx[100001];
 main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);
      cin>>n;
      for(int i=0;i<n;i++)cin>>vx[i];

      sort(vx,vx+n);
      long long int sum=0;
      for(int i=0;i<n;i++){
          sum+=abs(vx[n/2]-vx[i]);
      }
     cout<<sum;
}
